{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "scripts": {
      "preinstall": "npm install --legacy-peer-deps",
      "build": "npm run build",
      "start": "npm start"
    },
    "env": {
      "NODE_ENV": "production",
      "PUBLIC_URL": "."
    }
  }
  